<h2>Search Book</h2>
<form action="/books" method="GET">
<div class="">
  <label>Name</label>
  <input type="text" name="title" value="<%= searchOptions.name %>">
</div>
<div class="">
  <label>Published After</label>
  <input type="date" name="publishedAfter" value="<%= searchOptions.publishedAfter %>">
</div>
<div class="">
  <label>Published Before</label>
  <input type="date" name="publishedBefore" value="<%= searchOptions.publishedBefore %>">
</div>

  <button type="submit">Search</button>
</form>
<div class="">
  <% books.forEach(book => { %>
    <img src="<%= book.coverImagePath %>" alt="" height="150" width="100">
    <div><%= book.title %></div>
  <% }) %>
</div>
